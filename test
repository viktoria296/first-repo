
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication4
{
    class Program
    {
        static void Main(string[] args)
        {
            for (var i = 1; i <= 1000; i++)
            {
                
                if (i % 10 == 7)
                {

                    Console.Write(i + " ");
                }  
            }
        }
    }
}
